      "types": "./dist/generated/icons/RegardioSystemCircleIcon.d.ts"
    },
    "./icons/system-facet": {
      "import": "./dist/generated/icons/RegardioSystemFacetIcon.js",
      "types": "./dist/generated/icons/RegardioSystemFacetIcon.d.ts"
    },
    "./icons/system-plan": {
      "import": "./dist/generated/icons/RegardioSystemPlanIcon.js",
