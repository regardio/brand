{
  "$schema": "https://www.schemastore.org/package.json",
  "author": "Bernd Matzner <bernd.matzner@regard.io>",
  "bugs": {
    "url": "https://github.com/regardio/brand/issues"
  },
  "description": "Regardio Brand Assets",
  "devDependencies": {
    "@regardio/dev": "1.9.4",
    "@storybook/addon-a11y": "10.1.11",
    "@storybook/addon-docs": "10.1.11",
    "@storybook/addon-vitest": "10.1.11",
    "@storybook/react-vite": "10.1.11",
    "@svgr/cli": "8.1.0",
    "@svgr/plugin-jsx": "8.1.0",
    "@svgr/plugin-svgo": "8.1.0",
    "@tailwindcss/vite": "4.1.18",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "25.0.3",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@vitejs/plugin-react": "5.1.2",
    "@vitest/browser-playwright": "^4.0.16",
    "@vitest/coverage-v8": "^4.0.16",
    "linkedom": "0.18.12",
    "node-html-parser": "7.0.1",
    "playwright": "^1.57.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "storybook": "10.1.11",
    "svgo": "4.0.0",
    "tailwindcss": "4.1.18",
    "tsup": "8.5.1",
    "typescript": "5.9.3",
    "vite": "7.3.0",
    "vitest": "^4.0.16"
  },
  "exports": {
    "./colors.css": {
      "default": "./src/styles/colors.css"
    },
    "./icons/compass": {
      "import": "./dist/generated/icons/RegardioCompassIcon.js",
      "types": "./dist/generated/icons/RegardioCompassIcon.d.ts"
    },
    "./icons/ensemble-color": {
      "import": "./dist/generated/icons/RegardioEnsembleIconColor.js",
      "types": "./dist/generated/icons/RegardioEnsembleIconColor.d.ts"
    },
    "./icons/ensemble-color-full": {
      "import": "./dist/generated/icons/RegardioEnsembleIconColorFull.js",
      "types": "./dist/generated/icons/RegardioEnsembleIconColorFull.d.ts"
    },
    "./icons/ensemble-mono": {
      "import": "./dist/generated/icons/RegardioEnsembleIconMono.js",
      "types": "./dist/generated/icons/RegardioEnsembleIconMono.d.ts"
    },
    "./icons/regardio-color": {
      "import": "./dist/generated/icons/RegardioIconColor.js",
      "types": "./dist/generated/icons/RegardioIconColor.d.ts"
    },
    "./icons/regardio-color-full": {
