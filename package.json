    },
    "./icons/system-spark": {
      "import": "./dist/generated/icons/RegardioSystemSparkIcon.js",
      "types": "./dist/generated/icons/RegardioSystemSparkIcon.d.ts"
    },
    "./logos/regardio-logo-color": {
      "import": "./dist/generated/icons/RegardioLogoColor.js",
      "types": "./dist/generated/icons/RegardioLogoColor.d.ts"
    },
    "./logos/regardio-logo-color-light": {
      "import": "./dist/generated/icons/RegardioLogoColorLight.js",
