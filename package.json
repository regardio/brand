      "types": "./dist/generated/icons/RegardioIconMono.d.ts"
    },
    "./icons/system": {
      "import": "./dist/generated/icons/RegardioSystemIcon.js",
      "types": "./dist/generated/icons/RegardioSystemIcon.d.ts"
    },
    "./icons/system-bridge": {
